{
  "functions": {
    "src/app/**/*.{js,ts,jsx,tsx}": {
      "maxDuration": 60
    },
    "src/app/api/upload/**/*.{js,ts,jsx,tsx}": {
      "maxDuration": 300,
      "memory": 1024
    }
  },
  "regions": ["icn1"],
  "headers": [
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "buildCommand": "npm run build"
}
